import parser
import streams
import references

filename = "test/test.hm"
parser.set_input_dir("test")
parser.set_global_output_dir("out")
references.unserialize_link_references(filename)
parser.global_parse(streams.fileStream(filename))
references.serialize_link_references(filename)

print "\nFinal references:"
print "_references",references._references,"\n"
print "_id_to_reference_name",references._id_to_reference_name

if references._labels_changed :
    print "\n***Run again to get labels right.***\n"
